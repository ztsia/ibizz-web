{
  "_id": "form_c_2025_example",
  "formName": "Company Return Form C (Example)",
  "yearOfAssessment": 2025,
  "sections": [
    {
      "part": "Basic_Particulars",
      "title": "Basic Particulars",
      "show_if": null,
      "fields": [
        { "id": "1", "label": "Name of company", "inputType": "text" },
        { "id": "2", "label": "Reference no. (registration no.)", "inputType": "text" },
        { "id": "3", "label": "Tax Identification No. (TIN)", "inputType": "text" },
        { "id": "4", "label": "Incorporated in Malaysia", "inputType": "radio", "options": [{ "label": "Yes", "value": "1" }, { "label": "No", "value": "2" }] },
        { "id": "5", "label": "Labuan entity's TIN", "inputType": "text" },
        { "id": "6", "label": "Country of residence", "inputType": "countries" },
        { "id": "7", "label": "Employer's TIN", "inputType": "text" },
        { "id": "8", "label": "Status of business", "inputType": "radio", "options": [{ "label": "In operation", "value": "1" }, { "label": "Dormant", "value": "2" }, { "label": "In the process of winding up", "value": "3" }] },
        { "id": "9", "label": "Commencement date of operations (dd/mm/yyyy)", "inputType": "date" },
        { "id": "10_from", "label": "Accounting period from (dd/mm/yyyy)", "inputType": "date" },
        { "id": "10_to", "label": "Accounting period to (dd/mm/yyyy)", "inputType": "date" },
        { "id": "11_from", "label": "Basis period from (dd/mm/yyyy)", "inputType": "date" },
        { "id": "11_to", "label": "Basis period to (dd/mm/yyyy)", "inputType": "date" },
        { "id": "12", "label": "Company with paid-up ordinary share capital not exceeding RM2.5 million...", "inputType": "radio", "options": [{ "label": "Yes", "value": "1" }, { "label": "No", "value": "2" }] },
        { "id": "13", "label": "Company established for the issuance of asset-backed securities", "inputType": "radio", "options": [{ "label": "Yes", "value": "1" }, { "label": "No", "value": "2" }] },
        { "id": "14", "label": "Company limited by guarantee", "inputType": "radio", "options": [{ "label": "Yes", "value": "1" }, { "label": "No", "value": "2" }] },
        { "id": "15", "label": "Controlled company", "inputType": "radio", "options": [{ "label": "Yes", "value": "1" }, { "label": "No", "value": "2" }] },
        { "id": "16", "label": "Foreign company without share capital", "inputType": "radio", "options": [{ "label": "Yes", "value": "1" }, { "label": "No", "value": "2" }] },
        { "id": "17", "label": "Federal/State Statutory Body", "inputType": "radio", "options": [{ "label": "Yes", "value": "1" }, { "label": "No", "value": "2" }] },
        { "id": "18", "label": "Permanent Establishment in Malaysia", "inputType": "radio", "options": [{ "label": "Yes", "value": "1" }, { "label": "No", "value": "2" }] },
        { "id": "19", "label": "Substantial change in shareholding and subsection 44(5A) applies", "inputType": "radio", "options": [{ "label": "Yes", "value": "1" }, { "label": "No", "value": "2" }, { "label": "Not relevant", "value": "3" }] },
        { "id": "20", "label": "Claim/Surrender loss under the Group Relief provision", "inputType": "radio", "options": [{ "label": "Claim", "value": "1" }, { "label": "Surrender", "value": "2" }, { "label": "Not relevant", "value": "3" }] }
      ]
    },
    {
      "part": "G",
      "title": "Particulars of Company",
      "show_if": null,
      "fields": [
        { "id": "G1_line1", "label": "Correspondence address", "inputType": "text" },
        { "id": "G1_line2", "label": "Address Line 2", "inputType": "text", "isLabelHidden": true },
        { "id": "G1_line3", "label": "Address Line 3", "inputType": "text", "isLabelHidden": true },
        { "id": "G1_postcode", "label": "Postcode", "inputType": "text" },
        { "id": "G1_town", "label": "Town", "inputType": "text" },
        { "id": "G1_state", "label": "State", "inputType": "states" },
        { "id": "G1_country", "label": "Country", "inputType": "countries" },
        { "id": "G2", "label": "Address in G1 is the tax agent's address", "inputType": "radio", "options": [{ "label": "Yes", "value": "1" }, { "label": "No", "value": "2" }] },
        { "id": "G3a", "label": "Method of payment for tax refund", "inputType": "select", "options": [{ "label": "Via own account in Malaysia", "value": "1" }, { "label": "Via DuitNow", "value": "2" }, { "label": "Via overseas account/a third party account", "value": "3" }] },
        { "id": "G3b", "label": "Name of bank", "inputType": "text", "show_if": { "fieldId": "G3a", "operator": "not_equals", "value": "2" } },
        { "id": "G3c", "label": "Bank account no.", "inputType": "text", "show_if": { "fieldId": "G3a", "operator": "not_equals", "value": "2" } },
        { "id": "G3d", "label": "Bank identification no.", "inputType": "text", "show_if": { "fieldId": "G3a", "operator": "not_equals", "value": "2" } },
        { "id": "G4", "label": "Particulars of company directors (Fill in the required appendix)", "inputType": "readonly_note" },
        { "id": "G5", "label": "Particulars of company's major shareholders (Fill in the required appendix)", "inputType": "readonly_note" },
        { "id": "G6", "label": "Information of company's beneficial owner (Fill in the required appendix)", "inputType": "readonly_note" },
        { "id": "G7a", "label": "The company makes dividend payments to individual shareholders in the basis period", "inputType": "radio", "options": [{ "label": "Yes", "value": "1" }, { "label": "No", "value": "2" }] },
        { "id": "G7b", "label": "The company issues dividend certificates/vouchers to individual shareholders", "inputType": "radio", "options": [{ "label": "Yes", "value": "1" }, { "label": "No", "value": "2" }], "show_if": { "fieldId": "G7a", "operator": "equals", "value": "1" } },
        { "id": "G7c", "label": "The number of individual shareholders who received dividends exceeding RM100,000 (State the number)", "inputType": "number", "show_if": { "fieldId": "G7a", "operator": "equals", "value": "1" } },
        { "id": "G8", "label": "Foreign equity in comparison with paid-up capital (%)", "inputType": "number" },
        { "id": "G9", "label": "Government Linked Company (GLC)", "inputType": "radio", "options": [{ "label": "Yes", "value": "1" }, { "label": "No", "value": "2" }] },
        { "id": "G10", "label": "Company listed on Bursa Malaysia", "inputType": "radio", "options": [{ "label": "Yes", "value": "1" }, { "label": "No", "value": "2" }] },
        { "id": "G11", "label": "Has holding company", "inputType": "radio", "options": [{ "label": "Yes", "value": "1" }, { "label": "No", "value": "2" }] },
        { "id": "G12", "label": "Carry out controlled transactions under section 139 and 140A", "inputType": "radio", "options": [{ "label": "Yes", "value": "1" }, { "label": "No", "value": "2" }] },
        { "id": "G13", "label": "Receive income from sources outside Malaysia which is exempted from tax", "inputType": "radio", "options": [{ "label": "Yes", "value": "1" }, { "label": "No", "value": "2" }] },
        { "id": "G14", "label": "Subject to interest restriction under section 140C", "inputType": "radio", "options": [{ "label": "Yes", "value": "1" }, { "label": "No", "value": "2" }] },
        { "id": "G15a", "label": "Has Advance Ruling", "inputType": "radio", "options": [{ "label": "Yes", "value": "1" }, { "label": "No", "value": "2" }] },
        { "id": "G15b", "label": "Compliance with Advance Ruling", "inputType": "radio", "options": [{ "label": "Yes", "value": "1" }, { "label": "No", "value": "2" }], "show_if": { "fieldId": "G15a", "operator": "equals", "value": "1" } },
        { "id": "G15c", "label": "Material difference in tax arrangement compared to Advance Ruling", "inputType": "radio", "options": [{ "label": "Yes", "value": "1" }, { "label": "No", "value": "2" }], "show_if": { "fieldId": "G15a", "operator": "equals", "value": "1" } },
        { "id": "G16a", "label": "Has Advance Pricing Arrangement", "inputType": "radio", "options": [{ "label": "Yes", "value": "1" }, { "label": "No", "value": "2" }] },
        { "id": "G16b", "label": "Compliance with Advance Pricing Arrangement", "inputType": "radio", "options": [{ "label": "Yes", "value": "1" }, { "label": "No", "value": "2" }], "show_if": { "fieldId": "G16a", "operator": "equals", "value": "1" } },
        { "id": "G16c", "label": "Material difference in tax arrangement compared to Advance Pricing Arrangement", "inputType": "radio", "options": [{ "label": "Yes", "value": "1" }, { "label": "No", "value": "2" }], "show_if": { "fieldId": "G16a", "operator": "equals", "value": "1" } },
        { "id": "G17", "label": "Financial particulars of company (Fill in the required appendix)", "inputType": "readonly_note" },
        { "id": "G18", "label": "Has subsidiary company(ies) or related entity(ies) in the Federal Territory of Labuan", "inputType": "radio", "options": [{ "label": "Yes", "value": "1" }, { "label": "No", "value": "2" }] },
        { "id": "G19", "label": "Made payments to Labuan entity(ies)", "inputType": "radio", "options": [{ "label": "Yes", "value": "1" }, { "label": "No", "value": "2" }] }
      ]
    },
    {
      "part": "H",
      "title": "Other Particulars",
      "show_if": null,
      "fields": [
        { "id": "H1_type", "label": "Type of Income (for preceding year)", "inputType": "text" },
        { "id": "H1_year", "label": "Year of Assessment (for preceding year)", "inputType": "number" },
        { "id": "H1_amount", "label": "Amount (for preceding year)", "inputType": "number" },
        { "id": "H2", "label": "Made payments in the basis period which are subject to withholding tax...", "inputType": "radio", "options": [{ "label": "Yes", "value": "1" }, { "label": "No", "value": "2" }] },
        { "id": "H3a", "label": "Disposal of capital asset under Income Tax Act 1967", "inputType": "radio", "options": [{ "label": "Yes", "value": "1" }, { "label": "No", "value": "2" }] },
        { "id": "H3b", "label": "Disposal of capital asset declared to IRBM", "inputType": "radio", "options": [{ "label": "Yes", "value": "1" }, { "label": "No", "value": "2" }], "show_if": { "fieldId": "H3a", "operator": "equals", "value": "1" } },
        { "id": "H4a", "label": "Disposal of asset under the Real Property Gains Tax Act 1976", "inputType": "radio", "options": [{ "label": "Yes", "value": "1" }, { "label": "No", "value": "2" }] },
        { "id": "H4b", "label": "Disposal of asset declared to IRBM", "inputType": "radio", "options": [{ "label": "Yes", "value": "1" }, { "label": "No", "value": "2" }], "show_if": { "fieldId": "H4a", "operator": "equals", "value": "1" } },
        { "id": "H5a", "label": "Carry on E-commerce", "inputType": "radio", "options": [{ "label": "Yes", "value": "1" }, { "label": "No", "value": "2" }] },
        { "id": "H5b", "label": "E-Commerce business model", "inputType": "checkbox", "options": [{ "label": "Online sales/services", "value": "sales" }, { "label": "Online advertising", "value": "ads" }, { "label": "Cloud computing", "value": "cloud" }, { "label": "Payment services", "value": "payment" }, { "label": "Digital currency/digital token", "value": "digital" }, { "label": "e-Hailing/p-Hailing", "value": "hailing" }, { "label": "Other", "value": "other" }], "show_if": { "fieldId": "H5a", "operator": "equals", "value": "1" } },
        { "id": "H6a", "label": "Subject to Income Tax (Country-by-Country Reporting) Rules 2016", "inputType": "radio", "options": [{ "label": "Yes", "value": "1" }, { "label": "No", "value": "2" }] },
        { "id": "H6b", "label": "Subject as a reporting entity/non-reporting entity", "inputType": "radio", "options": [{ "label": "Reporting entity", "value": "1" }, { "label": "Non-reporting entity", "value": "2" }], "show_if": { "fieldId": "H6a", "operator": "equals", "value": "1" } },
        { "id": "H7", "label": "Has financial account(s) at financial institution(s) outside Malaysia", "inputType": "radio", "options": [{ "label": "Yes", "value": "1" }, { "label": "No", "value": "2" }] },
        { "id": "H8a", "label": "Company has implemented tax governance framework", "inputType": "radio", "options": [{ "label": "Yes", "value": "1" }, { "label": "No", "value": "2" }] },
        { "id": "H8b", "label": "Company participates in IRBM's tax governance program", "inputType": "radio", "options": [{ "label": "Yes", "value": "1" }, { "label": "No", "value": "2" }], "show_if": { "fieldId": "H8a", "operator": "equals", "value": "1" } },
        { "id": "H9a", "label": "Subject to Domestic Top-up Tax or Multinational Top-up Tax...", "inputType": "radio", "options": [{ "label": "Yes", "value": "1" }, { "label": "No", "value": "2" }] },
        { "id": "H9b", "label": "Name of the Ultimate Parent Entity and its Jurisdiction (Use country code)", "inputType": "text", "show_if": { "fieldId": "H9a", "operator": "equals", "value": "1" } }
      ]
    }
  ]
}