[
  {
    "_id": "form_c_2025_example",
    "formName": "Company Return Form C (Example)",
    "yearOfAssessment": 2025,
    "sections": [
      {
        "part": "Basic_Particulars",
        "title": "Basic Particulars",
        "show_if": null,
        "fields": [
          { "id": "company_name", "label": "Name of company", "inputType": "text", "required": true },
          { "id": "ref_no", "label": "Reference no. (registration no.)", "inputType": "text", "required": true },
          { "id": "company_tin", "label": "Tax Identification No. (TIN)", "inputType": "text", "required": true },
          { "id": "incorporated_malaysia", "label": "Incorporated in Malaysia", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }], "required": true },
          { "id": "labuan_tin", "label": "Labuan entity's TIN", "inputType": "text" },
          { "id": "country_residence", "label": "Country of residence", "inputType": "countries", "required": true },
          { "id": "employer_tin", "label": "Employer's TIN", "inputType": "text", "required": true },
          { "id": "business_status", "label": "Status of business", "inputType": "select", "options": [{ "label": "In operation", "value": "in_operation" }, { "label": "Dormant", "value": "dormant" }, { "label": "In the process of winding up", "value": "winding_up" }], "required": true },
          { "id": "commence_date", "label": "Commencement date of operations (dd/mm/yyyy)", "inputType": "date", "required": true },
          { "id": "acc_period_from", "label": "Accounting period from (dd/mm/yyyy)", "inputType": "date", "required": true },
          { "id": "acc_period_to", "label": "Accounting period to (dd/mm/yyyy)", "inputType": "date", "required": true },
          { "id": "basis_period_from", "label": "Basis period from (dd/mm/yyyy)", "inputType": "date", "required": true },
          { "id": "basis_period_to", "label": "Basis period to (dd/mm/yyyy)", "inputType": "date", "required": true },
          { "id": "small_capital", "label": "Company with paid-up ordinary share capital not exceeding RM2.5 million at the beginning of the basis period, gross income from all business sources not exceeding RM50 million and fulfils the requirements under paragraph 2B and 2C, Part 1, Schedule 1", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }], "required": true },
          { "id": "asset_backed", "label": "Company established for the issuance of asset-backed securities", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }], "required": true },
          { "id": "limited_by_guarantee", "label": "Company limited by guarantee", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }], "required": true },
          { "id": "controlled_company", "label": "Controlled company", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }], "required": true },
          { "id": "foreign_no_share", "label": "Foreign company without share capital", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }], "required": true },
          { "id": "statutory_body", "label": "Federal/State Statutory Body", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }], "required": true },
          { "id": "permanent_establishment", "label": "Permanent Establishment in Malaysia", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }], "required": true },
          { "id": "shareholding_change", "label": "Substantial change in shareholding and subsection 44(5A) applies", "inputType": "select", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }, { "label": "Not relevant", "value": "not_relevant" }], "required": true },
          { "id": "group_relief", "label": "Claim/Surrender loss under the Group Relief provision", "inputType": "select", "options": [{ "label": "Claim", "value": "claim" }, { "label": "Surrender", "value": "surrender" }, { "label": "Not relevant", "value": "not_relevant" }], "required": true }
        ]
      },
      {
        "part": "G",
        "title": "Particulars of Company",
        "show_if": null,
        "fields": [
          { "id": "correspondence_address", "label": "Correspondence address", "inputType": "text", "required": true },
          { "id": "address_line2", "label": "Address Line 2", "inputType": "text", "isLabelHidden": true },
          { "id": "address_line3", "label": "Address Line 3", "inputType": "text", "isLabelHidden": true },
          { "id": "postcode", "label": "Postcode", "inputType": "text", "required": true },
          { "id": "town", "label": "Town", "inputType": "text" },
          { "id": "state", "label": "State", "inputType": "states" },
          { "id": "country", "label": "Country", "inputType": "countries", "required": true },
          { "id": "tax_agent_address", "label": "Address in G1 is the tax agent's address", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }], "required": true },
          { "id": "tax_refund_method", "label": "Method of payment for tax refund", "inputType": "select", "options": [{ "label": "Via own account in Malaysia", "value": "own_account" }, { "label": "Via DuitNow", "value": "duitnow" }, { "label": "Via overseas account/a third party account", "value": "overseas_third_party" }], "required": true },
          { "id": "bank_name", "label": "Name of bank", "inputType": "text", "show_if": { "fieldId": "tax_refund_method", "operator": "not_equals", "value": "duitnow" }, "required": true },
          { "id": "bank_account_no", "label": "Bank account no.", "inputType": "text", "show_if": { "fieldId": "tax_refund_method", "operator": "not_equals", "value": "duitnow" }, "required": true },
          { "id": "bank_id_no", "label": "Bank identification no.", "inputType": "text", "show_if": { "fieldId": "tax_refund_method", "operator": "not_equals", "value": "duitnow" }, "required": true },
          { "id": "directors_particulars", "label": "Particulars of company directors (Fill in the required appendix)", "inputType": "readonly_note" },
          { "id": "major_shareholders", "label": "Particulars of company's major shareholders (Fill in the required appendix)", "inputType": "readonly_note" },
          { "id": "beneficial_owner_info", "label": "Information of company's beneficial owner (Fill in the required appendix)", "inputType": "readonly_note" },
          { "id": "dividend_payments", "label": "The company makes dividend payments to individual shareholders in the basis period", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }], "required": true },
          { "id": "dividend_certificates", "label": "The company issues dividend certificates/vouchers to individual shareholders", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }], "show_if": { "fieldId": "dividend_payments", "operator": "equals", "value": "yes" }, "required": true },
          { "id": "dividend_exceeding_100k", "label": "The number of individual shareholders who received dividends exceeding RM100,000 (State the number)", "inputType": "number", "show_if": { "fieldId": "dividend_payments", "operator": "equals", "value": "yes" }, "required": true },
          { "id": "foreign_equity_pct", "label": "Foreign equity in comparison with paid-up capital (%)", "inputType": "number", "required": true },
          { "id": "glc", "label": "Government Linked Company (GLC)", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }], "required": true },
          { "id": "listed_bursa", "label": "Company listed on Bursa Malaysia", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }], "required": true },
          { "id": "has_holding_company", "label": "Has holding company", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }], "required": true },
          { "id": "controlled_transactions", "label": "Carry out controlled transactions under section 139 and 140A", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }], "required": true },
          { "id": "interest_restriction", "label": "Subject to interest restriction under section 140C", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }], "required": true },
          { "id": "advance_ruling", "label": "Has Advance Ruling", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }], "required": true },
          { "id": "apa", "label": "Has Advance Pricing Arrangement", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }], "required": true },
          { "id": "apa_compliance", "label": "Compliance with Advance Pricing Arrangement", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }], "show_if": { "fieldId": "apa", "operator": "equals", "value": "yes" }, "required": true },
          { "id": "apa_difference", "label": "Material difference in tax arrangement compared to Advance Pricing Arrangement", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }], "show_if": { "fieldId": "apa", "operator": "equals", "value": "yes" }, "required": true },
          { "id": "financial_appendix", "label": "Financial particulars of company (Fill in the required appendix)", "inputType": "readonly_note" },
          { "id": "labuan_subsidiary", "label": "Has subsidiary company(ies) or related entity(ies) in the Federal Territory of Labuan", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }], "required": true },
          { "id": "labuan_payment", "label": "Made payments to Labuan entity(ies)", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }], "required": true }
        ]
      },
      {
        "part": "H",
        "title": "Other Particulars",
        "show_if": null,
        "fields": [
          { "id": "preceding_income_type", "label": "Type of Income (for preceding year)", "inputType": "text", "required": true },
            { "id": "preceding_income_ya", "label": "Year of Assessment (for preceding year)", "inputType": "number", "required": true },
            { "id": "preceding_income_amount", "label": "Amount (for preceding year)", "inputType": "number", "required": true },
            { "id": "withholding_payments", "label": "Made payments in the basis period which are subject to withholding tax under section 107A, 107B, 109, 109A, 109B, 109E, 109F and 109G", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }], "required": true },
            { "id": "disposal_capital_performed", "label": "Disposal of capital asset under Income Tax Act 1967", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }], "required": true },
            { "id": "disposal_capital_declared", "label": "Disposal of capital asset declared to IRBM", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }], "show_if": { "fieldId": "disposal_capital_performed", "operator": "equals", "value": "yes" }, "required": true },
            { "id": "disposal_rpgt_performed", "label": "Disposal of asset under the Real Property Gains Tax Act 1976", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }], "required": true },
            { "id": "disposal_rpgt_declared", "label": "Disposal of asset declared to IRBM", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }], "show_if": { "fieldId": "disposal_rpgt_performed", "operator": "equals", "value": "yes" }, "required": true },
            { "id": "ecommerce_performed", "label": "Carry on E-commerce", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }], "required": true },
            { "id": "ecommerce_model", "label": "E-Commerce business model", "inputType": "checkbox", "options": [{ "label": "Online sales/services", "value": "sales" }, { "label": "Online advertising", "value": "ads" }, { "label": "Cloud computing", "value": "cloud" }, { "label": "Payment services", "value": "payment" }, { "label": "Digital currency/digital token", "value": "digital" }, { "label": "e-Hailing/p-Hailing", "value": "hailing" }, { "label": "Other", "value": "other" }], "show_if": { "fieldId": "ecommerce_performed", "operator": "equals", "value": "yes" }, "required": true },
            { "id": "cbcr_subject_rules", "label": "Subject to Income Tax (Country-by-Country Reporting) Rules 2016", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }], "required": true },
            { "id": "cbcr_reporting_status", "label": "Subject as a reporting entity/non-reporting entity", "inputType": "radio", "options": [{ "label": "Reporting entity", "value": "reporting" }, { "label": "Non-reporting entity", "value": "non_reporting" }], "show_if": { "fieldId": "cbcr_subject_rules", "operator": "equals", "value": "yes" }, "required": true },
            { "id": "has_foreign_fin_accounts", "label": "Has financial account(s) at financial institution(s) outside Malaysia", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }], "required": true },
            { "id": "tax_governance", "label": "Company has implemented tax governance framework", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }], "required": true },
            { "id": "participates_tax_governance", "label": "Company participates in IRBM's tax governance program", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }], "show_if": { "fieldId": "tax_governance", "operator": "equals", "value": "yes" }, "required": true },
            { "id": "subject_to_topup", "label": "Subject to Domestic Top-up Tax or Multinational Top-up Tax under Part XI of the Income Tax Act 1967", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }], "required": true },
            { "id": "ultimate_parent_info", "label": "Name of the Ultimate Parent Entity and its Jurisdiction (Use country code)", "inputType": "text", "show_if": { "fieldId": "subject_to_topup", "operator": "equals", "value": "yes" }, "required": true }
        ]
      }
    ]
  }, 
  {
  "id": "page_controlled_transactions",
  "title": "Controlled Transactions",
  "show_if": {
    "fieldId": "controlled_transactions",
    "operator": "equals",
    "value": "yes"
  },
  "sections": [
    {
      "part": "CT_Holding_Company",
      "title": "Particulars of Holding Company",
      "show_if": null,
      "fields": [
                  {
                    "id": "ultimate_holding_name",
                    "label": "Ultimate Holding",
                    "inputType": "text",
                    "required": true
                  },          
                  {
                    "id": "ultimate_holding_country",
                    "label": "Ultimate Holding Country",
                    "inputType": "countries",
                    "required": true
                  },
                  {
                    "id": "ultimate_holding_tin",
                    "label": "Ultimate Holding Income Tax No.",
                    "inputType": "text",
                    "show_if": {
                      "fieldId": "ultimate_holding_country",
                      "operator": "equals",
                      "value": "MYS"
                    },
                    "required": true
                  },          
                  {
                    "id": "immediate_holding_name",
                    "label": "Immediate Holding",
                    "inputType": "text",
                    "required": true
                  },
                  {
                    "id":"immediate_holding_country",
                    "label": "Immediate Holding Country",
                    "inputType": "countries",
                    "required": true
                  },          
                  {
                    "id": "immediate_holding_tin",
                    "label": "Immediate Holding Income Tax No.",
                    "inputType": "text",
                    "show_if": {
                      "fieldId": "immediate_holding_country",
                      "operator": "equals",
                      "value": "MYS"
                    },
                    "required": true
                  }
                ]
    },
    {
      "part": "CT_Part_A",
      "title": "Part A: Company Information",
      "show_if": null,
      "fields": [
        {
          "id": "tp_documentation",
          "label": "Is the company required to prepare a Contemporaneous Transfer Pricing Documentation?",
          "inputType": "select",
          "options": [
            { "value": "exempted", "label": "Exempted" },
            { "value": "minimum", "label": "Minimum Contemporaneous Transfer Pricing Documentation" },
            { "value": "full", "label": "Full Contemporaneous Transfer Pricing Documentation" }
          ],
          "required": true
        },
        {
          "id": "manufacturing_performed",
          "label": "Manufacturing activities performed",
          "inputType": "radio",
          "options": [{ "value": "yes", "label": "Yes" }, { "value": "no", "label": "No" }],
          "required": true
        },
        {
          "id": "manufacturing_characterization",
          "label": "Characterization of manufacturing activity",
          "inputType": "select",
          "options": [
            { "value": "toll", "label": "Toll manufacturer" },
              { "value": "contract", "label": "Contract manufacturer" },
              { "value": "full_fledge", "label": "Full-fledge manufacturer" },
              { "value": "others", "label": "Others" }
            ],
            "show_if": { "fieldId": "manufacturing_performed", "operator": "equals", "value": "yes" },
            "required": true
        },
        {
          "id": "distribution_performed",
          "label": "Distribution activities performed",
          "inputType": "radio",
          "options": [{ "value": "yes", "label": "Yes" }, { "value": "no", "label": "No" }],
          "required": true
        },
        {
          "id": "distribution_characterization",
          "label": "Characterization of distribution activity",
          "inputType": "select",
          "options": [
            { "value": "full_fledge_distributor", "label": "Full-fledge distributor" },
            { "value": "commissionaire_distributor", "label": "Commissionaire distributor" },
            { "value": "agent_distributor", "label": "Agent distributor" },
            { "value": "limited_risk_distributor", "label": "Limited risk distributor" },
            { "value": "licensed_distributor", "label": "Licensed distributor" },
            { "value": "others", "label": "Others" }
          ],
          "show_if": { "fieldId": "distribution_performed", "operator": "equals", "value": "yes" },
          "required": true
        },
        {
          "id": "service_performed",
          "label": "Service activities performed",
          "inputType": "radio",
          "options": [{ "value": "yes", "label": "Yes" }, { "value": "no", "label": "No" }],
          "required": true
        },
        {
          "id": "service_characterization",
          "label": "Characterization of service activity",
          "inputType": "select",
          "options": [
            { "value": "management_service_provider", "label": "Management service provider" },
            { "value": "it_service_provider", "label": "IT service provider" },
            { "value": "treasury", "label": "Treasury" },
            { "value": "others", "label": "Others" }
          ],
          "show_if": { "fieldId": "service_performed", "operator": "equals", "value": "yes" },
          "required": true
        },
        {
          "id": "restructuring_performed",
          "label": "Business restructuring during the basis period",
          "inputType": "radio",
          "options": [{ "value": "yes", "label": "Yes" }, { "value": "no", "label": "No" }],
          "required": true
        },
        {
          "id": "restructuring_nature",
          "label": "State the nature of business restructuring",
          "inputType": "select",
          "options": [
            { "value": "full_to_toll", "label": "Conversion of full-fledge manufacturer into contract/toll manufacturer" },
            { "value": "toll_to_full", "label": "Conversion of contract/toll manufacturer into full-fledge manufacturer" },
            { "value": "full_to_limited_risk", "label": "Conversion of full-fledge distributor into limited risk/commissionaire/agent/licensed distributor" },
            { "value": "limited_risk_to_full", "label": "Conversion of limited risk/commissionaire/agent/licensed distributor into full-fledge distributor" },
            { "value": "ip_transfer_to_foreign", "label": "Transfer of intellectual property rights to foreign associated person" },
            { "value": "other", "label": "Other" }
          ],
          "show_if": { "fieldId": "restructuring_performed", "operator": "equals", "value": "yes" },
          "required": true
        },
        {
          "id": "rnd_performed",
          "label": "Performed any research and development activity",
          "inputType": "radio",
          "options": [{ "value": "yes", "label": "Yes" }, { "value": "no", "label": "No" }],
          "required": true
        },
        {
          "id": "rnd_type",
          "label": "State the types of R&D activity",
          "inputType": "select",
          "options": [
            { "value": "experimental_research", "label": "Systematic, investigative and experimental study that involves novelty" },
            { "value": "technical_risk", "label": "Technical risk carried out in the field of science or technology with the object of acquiring new knowledge" },
            { "value": "product_development", "label": "Study for the production or improvement of materials, devices, products, produce, or processes" },
            { "value": "other", "label": "Others" }
          ],
          "show_if": { "fieldId": "rnd_performed", "operator": "equals", "value": "yes" },
          "required": true
        },
        {
          "id": "owns_ip",
          "label": "Does the company own any trade/ brand name/ intellectual property?",
          "inputType": "radio",
          "options": [{ "value": "yes", "label": "Yes" }, { "value": "no", "label": "No" }],
          "required": true
        },
        {
          "id": "cash_pooling",
          "label": "Has the company involved in any cash pooling activities?",
          "inputType": "radio",
          "options": [{ "value": "yes", "label": "Yes" }, { "value": "no", "label": "No" }],
          "required": true
        },
        {
          "id": "cash_pool_role",
          "label": "Role in the cash pooling activity",
          "inputType": "radio",
          "options": [
            { "value": "leader", "label": "cash pool leader" },
            { "value": "member", "label": "cash pool member" }
          ],
          "show_if": { "fieldId": "cash_pooling", "operator": "equals", "value": "yes" },
          "required": true
        },
        {
          "id": "is_pe",
          "label": "Is the company a PE as defined under the TP Rules?",
          "inputType": "radio",
          "options": [{ "value": "yes", "label": "Yes" }, { "value": "no", "label": "No" }],
          "required": true
        },
        {
          "id": "pe_payments",
          "label": "Does the company make any interest, royalties and service payments to the company's head office? (for permanent establishment only)",
          "inputType": "radio",
          "options": [{ "value": "yes", "label": "Yes" }, { "value": "no", "label": "No" }],
          "show_if": { "fieldId": "is_pe", "operator": "equals", "value": "yes" },
          "required": true
        },
        {
          "id": "cca_involved",
          "label": "Has the company involved in any Cost Contribution Arrangement (CCA)",
          "inputType": "radio",
          "options": [{ "value": "yes", "label": "Yes" }, { "value": "no", "label": "No" }],
          "required": true
        }
      ]
    },
    {
      "part": "CT_Part_B",
      "title": "Part B: Controlled Transactions",
      "show_if": null,
      "fields": [
        {
          "id": "B_transactions",
          "label": "Controlled Transaction List",
          "inputType": "itemList",
          "itemStructure": {
            "key": {
              "id": "transaction_type",
              "label": "Transaction Type",
              "options": [
                { "value": "sales", "label": "Sales", "disabledFields": ["purchases_my", "purchases_os"] },
                { "value": "purchases", "label": "Purchases", "disabledFields": ["sales_my", "sales_os"] },
                { "value": "other_income", "label": "Other income", "disabledFields": ["purchases_my", "purchases_os"] },
                { "value": "intangibles_royalties", "label": "Payment on use of intangibles: Royalties" },
                { "value": "intangibles_license_fees", "label": "Payment on use of intangibles: License fees" },
                { "value": "intangibles_other_payments", "label": "Payment on use of intangibles: Other payments" },
                { "value": "management_fees", "label": "Management fees including fees /charges for financial, administrative, marketing and training services" },
