[
  {
    "_id": "form_c_2025_example",
    "formName": "Company Return Form C (Example)",
    "yearOfAssessment": 2025,
    "sections": [
      {
        "part": "Basic_Particulars",
        "title": "Basic Particulars",
        "show_if": null,
        "fields": [
          { "id": "company_name", "label": "Name of company", "inputType": "text" },
          { "id": "ref_no", "label": "Reference no. (registration no.)", "inputType": "text" },
          { "id": "company_tin", "label": "Tax Identification No. (TIN)", "inputType": "text" },
          { "id": "incorporated_malaysia", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }] },
          { "id": "labuan_tin", "label": "Labuan entity's TIN", "inputType": "text" },
          { "id": "country_residence", "label": "Country of residence", "inputType": "countries" },
          { "id": "employer_tin", "label": "Employer's TIN", "inputType": "text" },
          { "id": "business_status", "label": "Status of business", "inputType": "select", "options": [{ "label": "In operation", "value": "in_operation" }, { "label": "Dormant", "value": "dormant" }, { "label": "In the process of winding up", "value": "winding_up" }] },
          { "id": "commence_date", "label": "Commencement date of operations (dd/mm/yyyy)", "inputType": "date" },
          { "id": "acc_period_from", "label": "Accounting period from (dd/mm/yyyy)", "inputType": "date" },
          { "id": "acc_period_to", "label": "Accounting period to (dd/mm/yyyy)", "inputType": "date" },
          { "id": "basis_period_from", "label": "Basis period from (dd/mm/yyyy)", "inputType": "date" },
          { "id": "basis_period_to", "label": "Basis period to (dd/mm/yyyy)", "inputType": "date" },
          { "id": "small_capital", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }] },
          { "id": "asset_backed", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }] },
          { "id": "limited_by_guarantee", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }] },
          { "id": "controlled_company", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }] },
          { "id": "foreign_no_share", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }] },
          { "id": "statutory_body", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }] },
          { "id": "permanent_establishment", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }] },
          { "id": "shareholding_change", "inputType": "select", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }, { "label": "Not relevant", "value": "not_relevant" }] },
          { "id": "group_relief", "inputType": "select", "options": [{ "label": "Claim", "value": "claim" }, { "label": "Surrender", "value": "surrender" }, { "label": "Not relevant", "value": "not_relevant" }] }
        ]
      },
      {
        "part": "G",
        "title": "Particulars of Company",
        "show_if": null,
        "fields": [
          { "id": "correspondence_address", "label": "Correspondence address", "inputType": "text" },
          { "id": "address_line2", "label": "Address Line 2", "inputType": "text", "isLabelHidden": true },
          { "id": "address_line3", "label": "Address Line 3", "inputType": "text", "isLabelHidden": true },
          { "id": "postcode", "label": "Postcode", "inputType": "text" },
          { "id": "town", "label": "Town", "inputType": "text" },
          { "id": "state", "label": "State", "inputType": "states" },
          { "id": "country", "label": "Country", "inputType": "countries" },
          { "id": "tax_agent_address", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }] },
          { "id": "tax_refund_method", "inputType": "select", "options": [{ "label": "Via own account in Malaysia", "value": "own_account" }, { "label": "Via DuitNow", "value": "duitnow" }, { "label": "Via overseas account/a third party account", "value": "overseas_third_party" }] },
          { "id": "bank_name", "label": "Name of bank", "inputType": "text", "show_if": { "fieldId": "tax_refund_method", "operator": "not_equals", "value": "duitnow" } },
          { "id": "bank_account_no", "label": "Bank account no.", "inputType": "text", "show_if": { "fieldId": "tax_refund_method", "operator": "not_equals", "value": "duitnow" } },
          { "id": "bank_id_no", "label": "Bank identification no.", "inputType": "text", "show_if": { "fieldId": "tax_refund_method", "operator": "not_equals", "value": "duitnow" } },
          { "id": "directors_particulars", "label": "Particulars of company directors (Fill in the required appendix)", "inputType": "readonly_note" },
          { "id": "major_shareholders", "label": "Particulars of company's major shareholders (Fill in the required appendix)", "inputType": "readonly_note" },
          { "id": "beneficial_owner_info", "label": "Information of company's beneficial owner (Fill in the required appendix)", "inputType": "readonly_note" },
          { "id": "dividend_payments", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }] },
          { "id": "dividend_certificates", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }], "show_if": { "fieldId": "dividend_payments", "operator": "equals", "value": "yes" } },
          { "id": "dividend_exceeding_100k", "label": "The number of individual shareholders who received dividends exceeding RM100,000 (State the number)", "inputType": "number", "show_if": { "fieldId": "dividend_payments", "operator": "equals", "value": "yes" } },
          { "id": "foreign_equity_pct", "label": "Foreign equity in comparison with paid-up capital (%)", "inputType": "number" },
          { "id": "glc", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }] },
          { "id": "listed_bursa", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }] },
          { "id": "has_holding_company", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }] },
          { "id": "controlled_transactions", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }] },
          { "id": "interest_restriction", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }] },
          { "id": "advance_ruling", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }] },
          { "id": "advance_ruling_compliance", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }], "show_if": { "fieldId": "advance_ruling", "operator": "equals", "value": "yes" } },
          { "id": "advance_ruling_difference", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }], "show_if": { "fieldId": "advance_ruling", "operator": "equals", "value": "yes" } },
          { "id": "apa", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }] },
          { "id": "apa_compliance", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }], "show_if": { "fieldId": "apa", "operator": "equals", "value": "yes" } },
          { "id": "apa_difference", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }], "show_if": { "fieldId": "apa", "operator": "equals", "value": "yes" } },
          { "id": "financial_appendix", "label": "Financial particulars of company (Fill in the required appendix)", "inputType": "readonly_note" },
          { "id": "labuan_subsidiary", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }] },
          { "id": "labuan_payments", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }] }
        ]
      },
      {
        "part": "H",
        "title": "Other Particulars",
        "show_if": null,
        "fields": [
          { "id": "preceding_income_type", "label": "Type of Income (for preceding year)", "inputType": "text" },
          { "id": "preceding_income_ya", "label": "Year of Assessment (for preceding year)", "inputType": "number" },
          { "id": "preceding_income_amount", "label": "Amount (for preceding year)", "inputType": "currency" },
          { "id": "withholding_payments", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }] },
          { "id": "disposal_capital_performed", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }] },
          { "id": "disposal_capital_declared", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }], "show_if": { "fieldId": "disposal_capital_performed", "operator": "equals", "value": "yes" } },
          { "id": "disposal_rpgt_performed", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }] },
          { "id": "disposal_rpgt_declared", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }], "show_if": { "fieldId": "disposal_rpgt_performed", "operator": "equals", "value": "yes" } },
          { "id": "ecommerce_performed", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }] },
          { "id": "ecommerce_model", "inputType": "checkbox", "options": [{ "label": "Online sales/services", "value": "sales" }, { "label": "Online advertising", "value": "ads" }, { "label": "Cloud computing", "value": "cloud" }, { "label": "Payment services", "value": "payment" }, { "label": "Digital currency/digital token", "value": "digital" }, { "label": "e-Hailing/p-Hailing", "value": "hailing" }, { "label": "Other", "value": "other" }], "show_if": { "fieldId": "ecommerce_performed", "operator": "equals", "value": "yes" } },
          { "id": "cbcr_subject_rules", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }] },
          { "id": "cbcr_reporting_status", "inputType": "radio", "options": [{ "label": "Reporting entity", "value": "reporting" }, { "label": "Non-reporting entity", "value": "non_reporting" }], "show_if": { "fieldId": "cbcr_subject_rules", "operator": "equals", "value": "yes" } },
          { "id": "has_foreign_fin_accounts", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }] },
          { "id": "tax_governance", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }] },
          { "id": "participates_tax_governance", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }], "show_if": { "fieldId": "tax_governance", "operator": "equals", "value": "yes" } },
          { "id": "subject_to_topup", "inputType": "radio", "options": [{ "label": "Yes", "value": "yes" }, { "label": "No", "value": "no" }] },
          { "id": "ultimate_parent_info", "label": "Name of the Ultimate Parent Entity and its Jurisdiction (Use country code)", "inputType": "text", "show_if": { "fieldId": "subject_to_topup", "operator": "equals", "value": "yes" } }
        ]
      }
    ]
  }, 
  {
  "id": "page_controlled_transactions",
  "title": "Controlled Transactions",
  "show_if": {
    "fieldId": "controlled_transactions",
    "operator": "equals",
    "value": "yes"
  },
  "sections": [
    {
      "part": "CT_Holding_Company",
      "title": "Particulars of Holding Company",
      "show_if": null,
      "fields": [
        {
          "id": "ultimate_holding_name",
          "label": "Ultimate Holding",
          "inputType": "text"
        },
        {
          "id": "ultimate_holding_country",
          "label": "Ultimate Holding Country",
          "inputType": "countries"
        },
        {
          "id": "ultimate_holding_tin",
          "label": "Ultimate Holding Income Tax No.",
          "inputType": "text",
          "show_if": {
            "fieldId": "ultimate_holding_country",
            "operator": "equals",
            "value": "MYS"
          }
        },
        {
          "id": "immediate_holding_name",
          "label": "Immediate Holding",
          "inputType": "text"
        },
        {
          "id":"immediate_holding_country",
          "label": "Immediate Holding Country",
          "inputType": "countries"
        },
        {
          "id": "immediate_holding_tin",
          "label": "Immediate Holding Income Tax No.",
          "inputType": "text",
          "show_if": {
            "fieldId": "immediate_holding_country",
            "operator": "equals",
            "value": "MYS"
          }
        }
      ]
    },
    {
      "part": "CT_Part_A",
      "title": "Part A: Company Information",
      "show_if": null,
      "fields": [
        {
          "id": "tp_documentation",
          "label": "Is the company required to prepare a Contemporaneous Transfer Pricing Documentation?",
          "inputType": "select",
          "options": [
            { "value": "exempted", "label": "Exempted" },
            { "value": "minimum", "label": "Minimum Contemporaneous Transfer Pricing Documentation" },
            { "value": "full", "label": "Full Contemporaneous Transfer Pricing Documentation" }
          ]
        },
        {
          "id": "manufacturing_performed",
          "label": "Manufacturing activities performed",
          "inputType": "radio",
          "options": [{ "value": "yes", "label": "Yes" }, { "value": "no", "label": "No" }]
        },
        {
          "id": "manufacturing_characterization",
          "label": "Characterization of manufacturing activity",
          "inputType": "select",
          "options": [
            { "value": "toll", "label": "Toll manufacturer" },
              { "value": "contract", "label": "Contract manufacturer" },
              { "value": "full_fledge", "label": "Full-fledge manufacturer" },
              { "value": "others", "label": "Others" }
            ],
            "show_if": { "fieldId": "manufacturing_performed", "operator": "equals", "value": "yes" }
        },
        {
          "id": "distribution_performed",
          "label": "Distribution activities performed",
          "inputType": "radio",
          "options": [{ "value": "yes", "label": "Yes" }, { "value": "no", "label": "No" }]
        },
        {
          "id": "distribution_characterization",
          "label": "Characterization of distribution activity",
          "inputType": "select",
          "options": [
            { "value": "full_fledge_distributor", "label": "Full-fledge distributor" },
            { "value": "commissionaire_distributor", "label": "Commissionaire distributor" },
            { "value": "agent_distributor", "label": "Agent distributor" },
            { "value": "limited_risk_distributor", "label": "Limited risk distributor" },
            { "value": "licensed_distributor", "label": "Licensed distributor" },
            { "value": "others", "label": "Others" }
          ],
          "show_if": { "fieldId": "distribution_performed", "operator": "equals", "value": "yes" }
        },
        {
          "id": "service_performed",
          "label": "Service activities performed",
          "inputType": "radio",
          "options": [{ "value": "yes", "label": "Yes" }, { "value": "no", "label": "No" }]
        },
        {
          "id": "service_characterization",
          "label": "Characterization of service activity",
          "inputType": "select",
          "options": [
            { "value": "management_service_provider", "label": "Management service provider" },
            { "value": "it_service_provider", "label": "IT service provider" },
            { "value": "treasury", "label": "Treasury" },
            { "value": "others", "label": "Others" }
          ],
          "show_if": { "fieldId": "service_performed", "operator": "equals", "value": "yes" }
        },
        {
          "id": "restructuring_performed",
          "label": "Business restructuring during the basis period",
          "inputType": "radio",
          "options": [{ "value": "yes", "label": "Yes" }, { "value": "no", "label": "No" }]
        },
        {
          "id": "restructuring_nature",
          "label": "State the nature of business restructuring",
          "inputType": "select",
          "options": [
            { "value": "full_to_toll", "label": "Conversion of full-fledge manufacturer into contract/toll manufacturer" },
            { "value": "toll_to_full", "label": "Conversion of contract/toll manufacturer into full-fledge manufacturer" },
            { "value": "full_to_limited_risk", "label": "Conversion of full-fledge distributor into limited risk/commissionaire/agent/licensed distributor" },
            { "value": "limited_risk_to_full", "label": "Conversion of limited risk/commissionaire/agent/licensed distributor into full-fledge distributor" },
            { "value": "ip_transfer_to_foreign", "label": "Transfer of intellectual property rights to foreign associated person" },
            { "value": "other", "label": "Other" }
          ],
          "show_if": { "fieldId": "restructuring_performed", "operator": "equals", "value": "yes" }
        },
        {
          "id": "rnd_performed",
          "label": "Performed any research and development activity",
          "inputType": "radio",
          "options": [{ "value": "yes", "label": "Yes" }, { "value": "no", "label": "No" }]
        },
        {
          "id": "rnd_type",
          "label": "State the types of R&D activity",
          "inputType": "select",
          "options": [
            { "value": "experimental_research", "label": "Systematic, investigative and experimental study that involves novelty" },
            { "value": "technical_risk", "label": "Technical risk carried out in the field of science or technology with the object of acquiring new knowledge" },
            { "value": "product_development", "label": "Study for the production or improvement of materials, devices, products, produce, or processes" },
            { "value": "other", "label": "Other" }
          ],
          "show_if": { "fieldId": "rnd_performed", "operator": "equals", "value": "yes" }
        },
        {
          "id": "owns_ip",
          "label": "Does the company own any trade/ brand name/ intellectual property?",
          "inputType": "radio",
          "options": [{ "value": "yes", "label": "Yes" }, { "value": "no", "label": "No" }]
        },
        {
          "id": "cash_pooling",
          "label": "Has the company involved in any cash pooling activities?",
          "inputType": "radio",
          "options": [{ "value": "yes", "label": "Yes" }, { "value": "no", "label": "No" }]
        },
        {
          "id": "cash_pool_role",
          "label": "Role in the cash pooling activity",
          "inputType": "radio",
          "options": [
            { "value": "leader", "label": "cash pool leader" },
            { "value": "member", "label": "cash pool member" }
          ],
          "show_if": { "fieldId": "cash_pooling", "operator": "equals", "value": "yes" }
        },
        {
          "id": "is_pe",
          "label": "Is the company a PE as defined under the TP Rules?",
          "inputType": "radio",
          "options": [{ "value": "yes", "label": "Yes" }, { "value": "no", "label": "No" }]
        },
        {
          "id": "pe_payments",
          "label": "Does the company make any interest, royalties and service payments to the company's head office? (for permanent establishment only)",
          "inputType": "radio",
          "options": [{ "value": "yes", "label": "Yes" }, { "value": "no", "label": "No" }],
          "show_if": { "fieldId": "is_pe", "operator": "equals", "value": "yes" }
        },
        {
          "id": "cca_involved",
          "label": "Has the company involved in any Cost Contribution Arrangement (CCA)",
          "inputType": "radio",
          "options": [{ "value": "yes", "label": "Yes" }, { "value": "no", "label": "No" }]
        }
      ]
    },
    {
      "part": "CT_Part_B",
      "title": "Part B: Controlled Transactions",
      "show_if": null,
      "fields": [
        {
          "id": "B_transactions",
          "label": "Controlled Transaction List",
          "inputType": "itemList",
          "itemStructure": {
            "key": {
              "id": "transaction_type",
              "label": "Transaction Type",
              "options": [
                { "value": "sales", "label": "Sales" },
                { "value": "purchases", "label": "Purchases" },
                { "value": "other_income", "label": "Other income" },
                { "value": "intangibles_royalties", "label": "Payment on use of intangibles: Royalties" },
                { "value": "intangibles_license_fees", "label": "Payment on use of intangibles: License fees" },
                { "value": "intangibles_other_payments", "label": "Payment on use of intangibles: Other payments" },
                { "value": "management_fees", "label": "Management fees including fees /charges for financial, administrative, marketing and training services" },
                { "value": "research_and_development", "label": "Research and development" },
                { "value": "advertising_marketing_promotion", "label": "Advertisement, marketing and promotion (AMP)" },
                { "value": "tangible_assets", "label": "Tangible assets" },
                { "value": "rent_lease", "label": "Rent/Lease of assets" },
                { "value": "loans_to_associated", "label": "Loans to associated person" },
                { "value": "loans_from_associated", "label": "Loans from associated person" },
                { "value": "interest_to_associated", "label": "Interest to associated person" },
                { "value": "interest_from_associated_remitted", "label": "Interest from associated person remitted to Malaysia" },
                { "value": "guarantee_fee", "label": "Guarantee fee" },
                { "value": "cca_amount", "label": "Cost Contribution Arrangement (CCA) amount" },
                { "value": "payments_head_office_interest", "label": "Payments to head office (only for PE): Interest" },
                { "value": "payments_head_office_royalties", "label": "Payments to head office (only for PE): Royalties" },
                { "value": "payments_head_office_service", "label": "Payments to head office (only for PE): Service payments" },
                { "value": "sale_transfer_ip", "label": "Sale/Transfer of Intangible Property (IP)" },
                { "value": "others", "label": "Others not specified above" }
              ]
            },
            "values": [
              {
                "id": "sales_my",
                "label": "Sales/Revenue (In Malaysia)",
                "inputType": "currency"
              },
              {
                "id": "purchases_my",
                "label": "Purchases/Expenditure (In Malaysia)",
                "inputType": "currency"
              },
              {
                "id": "sales_os",
                "label": "Sales/Revenue (Outside Malaysia)",
                "inputType": "currency"
              },
              {
                "id": "purchases_os",
                "label": "Purchases/Expenditure (Outside Malaysia)",
                "inputType": "currency"
              }
            ]
          }
        }
      ]
    }
  ]
}, 
{
  "id": "page_labuan_details",
  "title": "Labuan Details",
  "show_if": {
    "operator": "or",
    "conditions": [
      { "fieldId": "labuan_subsidiary", "operator": "equals", "value": "yes" },
      { "fieldId": "labuan_payments", "operator": "equals", "value": "yes" }
    ]
  },
  "sections": [
    {
      "part": "Labuan_Subsidiary_List",
      "title": "Subsidiary Companies or Related Entities in the Federal Territory of Labuan",
      "show_if": {
        "fieldId": "labuan_subsidiary",
        "operator": "equals",
        "value": "yes"
      },
      "fields": [
        {
          "id": "labuan_subsidiaries",
          "label": "labuan-entities",
          "inputType": "lookup"
        }
      ]
    },
    {
      "part": "Labuan_Payment_List",
